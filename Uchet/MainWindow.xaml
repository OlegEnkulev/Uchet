<Window x:Class="Uchet.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Uchet"
        mc:Ignorable="d"
        Title="Учет успеваемости и посещаемости студентов" Height="450" Width="900" Background="#201c26" WindowStyle="ToolWindow" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="65*"/>
            <ColumnDefinition x:Name="SecondFrameGrid" Width="35*"/>
        </Grid.ColumnDefinitions>

        <Frame Grid.Column="0" x:Name="MainFrame" NavigationUIVisibility="Hidden"/>
        <Frame Grid.Column="1" x:Name="SecondFrame" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
